begin
  sdx_alza

  testa_giu

  ssx_alza

  500 delay

  testa_su

  500 delay

  avdx_sinistra 

  avsx_sinistra

  testa_giu

  500 delay

  avdx_destra

  avsx_destra

  testa_su

  500 delay

  avdx_sinistra

  avsx_sinistra

  testa_giu

  500 delay

  avdx_destra

  avsx_destra

  testa_su

  300 delay

  ssx_abbassa

  sdx_abbassa

  500 delay

  testa_giu

  500 delay




  # altra sequenza
  ssx_alza

  testa_su

  500 delay

  testa_giu

  avsx_sinistra

  500 delay

  testa_su

  avsx_destra

  500 delay

  testa_giu

  avsx_sinistra

  500 delay

  testa_su

  avsx_destra

  500 delay

  testa_giu

  ssx_abbassa

  500 delay

  testa_su

  sdx_alza

  ssx_abbassa
 
  500 delay

  testa_giu

  sdx_abbassa

  ssx_alza

   500 delay

  testa_su

  sdx_alza

  ssx_abbassa

  500 delay

  testa_giu

  sdx_abbassa

  ssx_alza

  500 delay

  testa_su

  ssx_abbassa

  500 delay
repeat

sub sdx_alza
  9908 1 servo
  return

sub testa_giu
  5000 4 servo
  return

sub ssx_alza
  2804 3 servo
  return 

sub testa_su
  10000 4 servo
  return

sub avdx_sinistra
  7200 0 servo
  return

sub avsx_sinistra
  8960 2 servo
  return

sub avdx_destra
  3520 0 servo
  return

sub avsx_destra
  3982 2 servo
  return

sub ssx_abbassa
  8272 3 servo
  return

sub sdx_abbassa
  4744 1 servo
  return

sub avdx_reset
  4900 0 servo

sub avsx_reset
  6000 2 servo